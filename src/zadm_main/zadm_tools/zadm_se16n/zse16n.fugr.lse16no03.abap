MODULE SET_CUA_variant OUTPUT.

   set pf-status '0600'.
   case sy-dynnr.
     when '0600'.
       set titlebar '600'.
     when '0601'.
       set titlebar '601'.
     when '0602'.
       set titlebar '602'.
   endcase.

   text_2 = text-v04.

ENDMODULE.                 " SET_CUA_layout  OUTPUT
